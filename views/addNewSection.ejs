<%- include("partials/header") -%>


<form action="" method="post" style="margin:0 auto;width:50%;padding-top: 6%;">
  <div class="form-group">
    <label for="sectionName">Section Name</label>
    <input type="text" class="form-control" id="sectionName" name="sectionName" aria-describedby="sectionName"
      placeholder="Enter section name">
    <small id="emailHelp" class="form-text text-muted">Enter a new unique section you want to add..</small>
  </div>
  <button type="submit" class="btn btn-primary" style="background-color: rgb(99,209,187);" >Submit</button>
</form>



